    "next": "^14.1.0",
    "next-head-seo": "^0.1.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.33.1",
    "react-icons": "^5.0.1",
    "react-select": "^5.4.0",
    "recoil": "^0.7.5",
    "sass": "^1.53.0",
    "swr": "^2.2.5",
    "zod": "^3.20.6"
